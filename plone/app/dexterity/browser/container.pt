<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone.app.dexterity">
<body>

<metal:main fill-slot="content-core">
  <metal:content-core define-macro="content-core">

    <tal:block define="render_widgets context/@@render-widgets/render_widgets;
                       widgets view/widgets/values;">
        <metal:use_macro use-macro="render_widgets" />
    </tal:block>

    <tal:block define="render_groups context/@@render-groups/render_groups;
                       groups view/groups;">
        <metal:use_macro use-macro="render_groups" />
    </tal:block>

    <tal:block define="render_folder_listing context/@@render-folder-listing/render_folder_listing;">
        <metal:use_macro use-macro="render_folder_listing" />
    </tal:block>

  </metal:content-core>
</metal:main>

</body>
</html>
